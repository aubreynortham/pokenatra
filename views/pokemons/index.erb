<div class='containareon'>

  <h2>Pokemon <a href="/pokemons/new">(+)</a></h2>

  <% @pokemon.each do |pokemon| %>

  <h3>
    <a href='/pokemons/<%= pokemon.id %>'>
      <%= pokemon.name %>
    </a>
  </h3>

  <img class='pokemon' src='<%= pokemon.img_url %>'>

  <% end %>

  <h4>Trainers <a href="/trainers">(view all)</a></h4>
</div>
